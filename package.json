{
  "name": "evl-ui-components",
  "version": "1.0.0-alpha.19",
  "description": "Everledger UI Component Library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean-lib": "rimraf lib/*",
    "build-lib": "ttsc",
    "build": "npm run clean-lib && npm run build-lib",
    "build-lib-watch": "ttsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "storybook": "start-storybook -s public -p 6006",
    "build-storybook": "build-storybook"
  },
  "license": "MIT",
  "dependencies": {
    "@date-io/date-fns": "~1.3.13",
    "@material-ui/core": "~4.9.5",
    "@material-ui/icons": "~4.9.1",
    "@material-ui/lab": "~4.0.0-alpha.45",
    "@material-ui/pickers": "~3.2.10",
    "@types/lodash": "~4.14.149",
    "@types/react-pdf": "~4.0.3",
    "classnames": "~2.2.6",
    "date-fns": "~2.10.0",
    "formik": "~2.1.4",
    "lodash": "~4.17.15",
    "react-pdf": "~4.1.0"
  },
  "peerDependencies": {
    "react": "~16.13.0",
    "react-dom": "~16.13.0",
    "react-jss": "~10.0.4"
  },
  "devDependencies": {
    "@babel/core": "~7.8.6",
    "@storybook/addon-a11y": "~5.3.14",
    "@storybook/addon-actions": "~5.3.14",
    "@storybook/addon-info": "~5.3.14",
    "@storybook/addon-links": "~5.3.14",
    "@storybook/addons": "~5.3.14",
    "@storybook/react": "~5.3.14",
    "@types/classnames": "~2.2.9",
    "@types/jss": "~10.0.0",
    "@types/node": "~13.7.7",
    "@types/react": "~16.9.25",
    "@types/react-dom": "~16.9.5",
    "@types/react-jss": "~10.0.0",
    "@types/yup": "~0.26.32",
    "@zerollup/ts-transform-paths": "~1.7.17",
    "babel-loader": "~8.0.6",
    "husky": "4",
    "lint-staged": "10",
    "prettier": "~1.19.1",
    "react-docgen-typescript-loader": "~3.6.0",
    "rimraf": "~3.0.2",
    "ts-loader": "~6.2.1",
    "tsconfig-paths-webpack-plugin": "~3.2.0",
    "tslint": "~6.0.0",
    "tslint-config-prettier": "~1.18.0",
    "ttypescript": "~1.5.10",
    "typescript": "~3.8.3",
    "yup": "~0.28.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts?(x)": [
      "prettier --write",
      "git add"
    ]
  }
}
